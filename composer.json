{
    "name": "francis/recaptcha-bundle",
    "type": "symfony-bundle",
    "description": "ReCaptcha form field integration for Symfony",
    "keywords": ["recaptcha", "form", "google", "captcha"],
    "homepage": "https://github.com/francisbesset/FrancisReCaptchaBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Francis Besset",
            "email": "francis.besset@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "ext-curl": "*",
        "symfony/framework-bundle": "~2.7|~3.0",
        "symfony/form": "~2.7|~3.0",
        "symfony/validator": "~2.5|~3.0",
        "google/recaptcha": "~1.1,>=1.1.2",
        "psr/log": "~1.0"
    },
    "autoload": {
        "psr-4": { "Francis\\Bundle\\ReCaptchaBundle\\": "" }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
